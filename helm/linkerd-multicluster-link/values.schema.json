{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "commonLabels": {
            "type": "object"
        },
        "controllerImage": {
            "type": "string"
        },
        "controllerImageVersion": {
            "type": "string"
        },
        "enableHeadlessServices": {
            "type": "boolean"
        },
        "enablePSP": {
            "type": "boolean"
        },
        "gateway": {
            "type": "object",
            "properties": {
                "probe": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "registry": {
                    "type": "string"
                }
            }
        },
        "logLevel": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podLabels": {
            "type": "object"
        },
        "resources": {
            "type": "object"
        },
        "serviceMirrorRetryLimit": {
            "type": "integer"
        },
        "serviceMirrorUID": {
            "type": "integer"
        },
        "target": {
            "type": "object",
            "properties": {
                "CA": {
                    "type": "string"
                },
                "api": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "gateway": {
                    "type": "object",
                    "properties": {
                        "ip": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        },
                        "portProbe": {
                            "type": "string"
                        }
                    }
                },
                "token": {
                    "type": "string"
                }
            }
        },
        "targetClusterName": {
            "type": "string"
        },
        "tolerations": {
            "type": "object"
        }
    }
}
